<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="git.snippets.cloud.ms.mapper.Example2ProductMapper">


    <select id="queryGoodInfoByGoodId" resultType="git.snippets.cloud.ms.entity.Example2ProductEntity"
            parameterType="git.snippets.cloud.ms.bo.Example2ProductBo">
        select *
        from example2_product
        where good_id = #{goodId}
    </select>

</mapper>